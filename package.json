{
  "name": "gdxviewer",
  "displayName": "GDXViewer",
  "description": "VS Code extension for previewing GAMS GDX files",
  "publisher": "VaibhavnathJha",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vaibhavnath-Jha/vscode-gdxviewer.git"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "GAMS",
    "gdx"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gdx.Display",
        "title": "GDX: Display the .gdx file"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .gdx",
          "command": "gdx.Display",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
