{
  "name": "gdxviewer",
  "displayName": "gdxviewer",
  "description": "VSCode extension for previewing GAMS GDX files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:gdx"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gdx.Display",
        "title": "GDX: Display the .gdx file"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .gdx",
          "command": "gdx.Display",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "gdx",
        "extensions": [
          "*.gdx"
        ],
        "aliases": [
          "GDX"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}