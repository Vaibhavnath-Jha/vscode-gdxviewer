{
  "name": "gdxviewer",
  "displayName": "GDXViewer",
  "description": "VS Code extension for previewing GAMS GDX files",
  "publisher": "VaibhavnathJha",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vaibhavnath-Jha/vscode-gdxviewer.git"
  },
  "version": "1.2.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:gdx"
  ],
  "keywords": [
    "GAMS",
    "gdx"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "gdx",
        "extensions": [
          ".gdx"
        ]
      }
    ],
    "commands": [
      {
        "command": "gdx.Display",
        "title": "GDX Viewer: Display the .gdx file"
      },
      {
        "command": "gdx.initializeDevContainer",
        "title": "GDX Viewer: Create Recommended Dev Container File"
      }
    ],
    "customEditors": [
      {
        "viewType": "gdx.Display",
        "displayName": "GDX Viewer",
        "selector": [
          {
            "filenamePattern": "*.gdx"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  }
}
